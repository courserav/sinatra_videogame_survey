<div>
    <h2>See What Other People are Saying:</h2>
    <br>
    <% @surveys.each do |survey| %>
        <h4> <%= survey.id %>: <%= survey.console %>, <%= survey.genre %> </h4>
        <h5>Username: <%= survey.user.username unless survey.user.blank? %></h5>
        <p><a href="/surveys/<%= survey.id %>">View Survey Results</a></p>
        <br>
    <% end %>
</div>

